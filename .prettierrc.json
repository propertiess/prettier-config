{
  "tabWidth": 2,
  "printWidth": 80,
  "arrowParens": "avoid",
  "endOfLine": "crlf",
  "overrides": [
    {
      "files": ".*rc",
      "options": {
        "parser": "json"
      }
    }
  ],
  "semi": true,
  "trailingComma": "none",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "plugins": ["prettier-plugin-multiline-arrays"],
  "multilineArraysWrapThreshold": 5,
  "multilineArraysLinePattern": "2 2"
}